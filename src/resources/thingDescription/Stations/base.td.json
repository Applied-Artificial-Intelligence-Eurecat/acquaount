  "actions": {
    "receiveMeasure": {
      "safe": true,
      "idempotent": false,
      "input": {
        "type": "object",
        "description": "The message that the data retriever sends",
        "readOnly": true,
        "writeOnly": false,
        "properties": {
          "info": {
            "type": "object",
            "readOnly": true,
            "writeOnly": false,
            "description": "The necessary information to identify from which platform is this data coming from",
            "properties": {
